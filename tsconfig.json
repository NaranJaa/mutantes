{
  "extends": "@tsconfig/recommended/tsconfig.json",
  "compilerOptions": {
    "rootDirs": ["dist/*"],
    "baseUrl": ".",
    "paths": {
      "@/*": [ "src/*"],
      "@apiServices": ["apiServices/*"]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true
  }
}
